{
    "name": "afromansr/laravel-nira-api",
    "description": "An elegant way to access the NIRA api for the Laravel framework",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AfromanSR\\LaravelNiraApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AfromanSR\\LaravelNiraApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Samson Rwakabuguli",
            "email": "srwakabuguli@gmail.com"
        }
    ],
    "require": {
        "ext-soap": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AfromanSR\\LaravelNiraApi\\ServiceProvider"
            ]
        }
    }
}
